### 7. INTEGRATION TESTS
### ============================================

### 7.1 Complete Exam Flow
# Step 1: Login
# @name flowLogin
POST {{baseUrl}}/login
Content-Type: application/json

{
  "email": "test@example.com",
  "password": "password"
}

###

# Step 2: Get Exam Info
GET {{baseUrl}}/exams/1
Authorization: Bearer {{flowLogin.response.body.token}}

###

# Step 3: Get Exam Instructions
GET {{baseUrl}}/exams/1/instructions
Authorization: Bearer {{flowLogin.response.body.token}}

###

# Step 4: Get Questions
GET {{baseUrl}}/exams/1/questions
Authorization: Bearer {{flowLogin.response.body.token}}

###

# Step 5: Submit Answers
# @name flowSubmit
POST {{baseUrl}}/exam-results
Authorization: Bearer {{flowLogin.response.body.token}}
Content-Type: application/json

{
  "exam_id": 1,
  "user_id": 1,
  "answers": [
    "C. Gunung",
    "A. 1 & 3",
    "C. (4)",
    "A. Ketekunan Sururi",
    "B. Cekungan di antara pegunungan",
    "D. Semua benar",
    "A. Pertemuan daratan dengan laut",
    "D. Semua benar",
    "B. Cekungan berisi air tawar",
    "A. Aktivitas tektonik",
    "B. 200-1000 meter",
    "A. Daratan yang menjorok ke laut",
    "B. Laut yang menjorok ke daratan",
    "D. Semua benar",
    "A. Laut sempit di antara dua daratan",
    "B. Masih dapat meletus",
    "D. Semua benar",
    "B. Tempat tinggi ke tempat rendah",
    "A. Daratan yang dikelilingi air",
    "B. Sarana rekreasi"
  ],
  "start_time": "{{$datetime iso8601 -1 h}}",
  "end_time": "{{$datetime iso8601}}"
}

###

# Step 6: Get Result
GET {{baseUrl}}/exam-results/{{flowSubmit.response.body.result.id}}
Authorization: Bearer {{flowLogin.response.body.token}}

###

# Step 7: Get Review
GET {{baseUrl}}/exam-results/{{flowSubmit.response.body.result.id}}/review
Authorization: Bearer {{flowLogin.response.body.token}}

###

# Step 8: Get History
GET {{baseUrl}}/test-history
Authorization: Bearer {{flowLogin.response.body.token}}

